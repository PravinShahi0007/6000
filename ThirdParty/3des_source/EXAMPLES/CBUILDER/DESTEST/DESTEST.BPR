# ---------------------------------------------------------------------------
!if !$d(BCB)
BCB = $(MAKEDIR)\..
!endif

# ---------------------------------------------------------------------------
# IDE SECTION
# ---------------------------------------------------------------------------
# The following section of the project makefile is managed by the BCB IDE.
# It is recommended to use the IDE to change any of the values in this
# section.
# ---------------------------------------------------------------------------

VERSION = BCB.04.04
# ---------------------------------------------------------------------------
PROJECT = Destest.exe
OBJFILES = Destest.obj frmTest.obj
RESFILES = Destest.res
DEFFILE =
RESDEPEN = $(RESFILES) frmTest.dfm
LIBFILES =
LIBRARIES = dclusr40.lib vcl40.lib
SPARELIBS = vcl40.lib dclusr40.lib
PACKAGES = vcl40.bpi
# ---------------------------------------------------------------------------
PATHCPP = .;
PATHASM = .;
PATHPAS = .;
PATHRC = .;
DEBUGLIBPATH = $(BCB)\lib\debug
RELEASELIBPATH = $(BCB)\lib\release
SYSDEFINES = NO_STRICT
USERDEFINES = $(SYSDEFINES);$(USERDEFINES)
# ---------------------------------------------------------------------------
CFLAG1 = -I$(BCB)\Include;$(BCB)\Include\vcl;..\..\..\..\des -O2 -Hc \
  -H=$(BCB)\lib\vcl40.csm -w -Ve -Tkh30000 -fp -ff -a8 -5 -k -y -v -vi- \
  -D$(SYSDEFINES);$(USERDEFINES) -c -b- -w-par -w-inl -Vx -tW -tWM -tWM
CFLAG2 =
CFLAG3 =
PFLAGS = -U$(BCB)\Projects\Lib;$(BCB)\lib\obj;$(BCB)\lib;$(RELEASELIBPATH) \
  -I$(BCB)\Include;$(BCB)\Include\vcl;..\..\..\..\des \
  -D$(SYSDEFINES);$(USERDEFINES) -$YD -$W -$O- -$C- -v -M -JPHNE -JPHNE
RFLAGS = -i$(BCB)\Include;$(BCB)\Include\vcl;..\..\..\..\des \
  -D$(SYSDEFINES);$(USERDEFINES)
AFLAGS = /i$(BCB)\Include /i$(BCB)\Include\vcl /i..\..\.. /d$(SYSDEFINES) \
  /d$(USERDEFINES) /mx /w2 /zd
LFLAGS = -L$(BCB)\Projects\Lib;$(BCB)\lib\obj;$(BCB)\lib;$(RELEASELIBPATH) -aa -Tpe -x \
  -Gn -v IFLAGS =
IFLAGS =
# ---------------------------------------------------------------------------
ALLOBJ = c0w32.obj sysinit.obj sysinit.obj $(OBJFILES)
ALLRES = $(RESFILES)
ALLLIB = $(LIBFILES) $(LIBRARIES) import32.lib cp32mt.lib
# ---------------------------------------------------------------------------
!ifdef IDEOPTIONS

